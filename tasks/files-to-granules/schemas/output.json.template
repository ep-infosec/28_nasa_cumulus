{
  "title": "FilesToGranulesOutput",
  "description": "Describes the output produced by the files-to-granules task",
  "type": "object",
  "properties": {
    "granules": {
      "type": "array",
      "description": "Array of all granules",
      "items": {
        "type": "object",
        "required": ["granuleId", "files"],
        "properties": {
          "granuleId": {
            "type": "string"
          },
          "files": "{{files}}"
        }
      }
    }
  }
}
